<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<welcome-file-list>
		<welcome-file>
	       index.html 
	    </welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>q</servlet-name>
		<servlet-class>mybean.data.HandleLogin</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>w</servlet-name>
		<servlet-class>mybean.data.HandleRegister</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>w</servlet-name>
		<url-pattern>/helpRegister</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>q</servlet-name>
		<url-pattern>/loginservlet</url-pattern>
	</servlet-mapping>
</web-app>
